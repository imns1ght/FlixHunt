{
  "name": "movie-browser",
  "version": "0.0.1",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "android-reverse": "adb reverse tcp:8081 tcp:8081",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "eslint --fix . --ext .ts,.tsx",
    "build": "cd android/ && ./gradlew assembleRelease && cd ..",
    "format": "prettier --write ./src"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/stack": "^6.0.9",
    "axios": "^0.21.0",
    "react": "18.0.0",
    "react-dom": "17.0.2",
    "react-native": "0.69.1",
    "react-native-collapsible": "^1.6.0",
    "react-native-gesture-handler": "^2.4.0",
    "react-native-image-pan-zoom": "^2.1.12",
    "react-native-modal": "^13.0.1",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-webview": "^11.22.6",
    "react-native-youtube-iframe": "^2.2.2",
    "react-number-format": "^4.4.1",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "~7.15.5",
    "@react-native-community/cli-platform-android": "^8.0.2",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/react": "~17.0.24",
    "@types/react-dom": "~17.0.9",
    "@types/react-native": "~0.65.1",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-jest": "~27.2.2",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "~27.2.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "react-native-clean-project": "^3.6.7",
    "react-test-renderer": "~17.0.2",
    "reactotron-react-native": "^5.0.1",
    "typescript": "~4.4.3"
  },
  "resolutions": {
    "@types/react": "17.0.43"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
